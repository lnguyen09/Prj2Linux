<!-- The rendered result page after predicting-->
<!DOCTYPE html>

<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Home</title>
  <link rel="stylesheet" href="/stylesheets/style.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>

<body>
  <!-- Create a navbar-->
  <div class="topnav" id="myTopnav">
    <a href="/" class="active">Home</a>
    <a href="/textver">Text version</a>
    <a href="/about">About</a>
    <a href="javascript:void(0);" class="icon" onclick="">&#9776;</a>
  </div>

  <!--Render the result-->
  <div class="mt-5 mh-400 row align-items-center">
    <div class="mh-100" style="width: 100px; height: 200px;" >
    </div>
    <h2 class="text-center text-danger"><%= details.message2%></h2>
  </div>
  <div style="margin-top: 50px;">
    <h5 style="text-align: center;">Most recent 20 uses:</h5>
  </div>

  <!--The table showing the most recent 20 uses-->
  <table>
    <tr>
        <th>Input</th>
        <th>Result</th>
    </tr>
    <% records.forEach(function(item){ %>
    <tr>
        <td><%= item.Input %></td>
        <td><%= item.Result%></td>
    </tr>
    <% }) %>
  </table>
</body>
</html>
  